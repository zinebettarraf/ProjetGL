// Description:
//    tests sur les booleans
//
// Resultats:
//    a ou b ou les deux false 
//    a OU b sont true
//
// Historique:
//    cree le 15/01/2022

{

   
   boolean a = true ; 
   boolean b = false ;

   
   if (a && b ) {
       println("a AND b sont true");
   }
   else {
       println( "a ou b ou les deux false ");
   }

   if (a || b ) {
       println("a OU b sont true");
   }
   else {
       println( "a et b sont false" );
   }



}
